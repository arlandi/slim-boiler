{
    "name": "arlandi/slim-starter",
    "type": "application",
    "keywords": ["app","flatfile"],
    "license": "MIT",
    "version": "1.0.0",
    "description": "A flat-file engine for PHP",
    "homepage": "http://anggiarlandi.com",
    "authors": [{
            "name": "Anggi",
            "homepage": "http://anggiarlandi.com"
        }],
    "require": {
        "php": ">=5.3.3",
        "michelf/php-markdown" : "1.4.*",
        "slim/slim": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Textpress" : "src/shameerc"
        }
    },
    "scripts": {
        "run": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888"
        ]
    }
}
